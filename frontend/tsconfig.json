{
	"compilerOptions": {
		// For Angular
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true
	},
	"include": ["src/**/*.ts", "src/**/*.html", "src/**/*.css"],
	"exclude": ["node_modules", "dist", "e2e"],
	"noUnusedLocals": true,
	"noUnusedParameters": true,
	"noImplicitReturns": true
}
